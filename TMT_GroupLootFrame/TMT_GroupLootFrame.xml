<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

	<GameTooltip name="TMT_GLF_TooltipHidden" inherits="GameTooltipTemplate" parent="UIParent" hidden="true" />
	
	<Frame name="TMT_GLF_GroupLootFrameTemplate" hidden="true" virtual="true">
		<Size x="10" y="10" />
		<Layers>
			<Layer level="OVERLAY">
				<Texture name="$parentTexture" file="Interface\Minimap\TRACKING\OBJECTICONS">
					<Size>
						<AbsDimension x="32" y="32"/>
					</Size>
					<Anchors>
						<Anchor point="CENTER" relativePoint="TOPLEFT" />
					</Anchors>
					<TexCoords left="0.0" right="0.125" top="0" bottom="0.5"/>
				</Texture>
			</Layer>
		</Layers>
	</Frame>
	<Frame name="TMT_GLF_GroupLootFrame1" inherits="TMT_GLF_GroupLootFrameTemplate" parent="GroupLootFrame1" toplevel="true" id="1">
		<Anchors>
			<Anchor point="TOPLEFT" relativeTo="GroupLootFrame1IconFrameIcon" relativePoint="TOPLEFT">
				<Offset>
					<AbsDimension x="0" y="0"/>
				</Offset>
			</Anchor>
		</Anchors>
	</Frame>
	<Frame name="TMT_GLF_GroupLootFrame2" inherits="TMT_GLF_GroupLootFrameTemplate" parent="GroupLootFrame2" toplevel="true" id="2">
